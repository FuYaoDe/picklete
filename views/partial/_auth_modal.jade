// modal-control-shop-type-lg
#modal-control-shop-type-lg.modal.fade
  .modal-dialog(role='document')
    .modal-content
      .modal-header.p-30
        button.close(type='button', data-dismiss='modal', aria-label='Close')
          span(aria-hidden='true') ×
        h2.modal-title 大館別設定
      // /modal-header
      .modal-body.p-30.p-top-0.font-size-slarge
        form
          .form-group.m-bottom-6
            label 大館別名稱
            input.form-control(type='text')
          // /form-group
          a.btn.btn-danger(href='#', data-dismiss='modal') 確定
      // /modal-body
    // /modal-content
  // /modal-dialog
// /modal
