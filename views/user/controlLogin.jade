  body.control-panel-page
    //- .loader
    a#scrolltotop
      span.glyphicon.glyphicon-menu-up
    include ../partial/_auth_header
    .container.font-size-slarge
      .row.m-top-10.m-bottom-10
        .col-sm-6.col-sm-offset-3
          .border-block-3.p-40
            h2.text-center.m-bottom-4 管理員登入
            hr
            form.form-horizontal
              .form-group
                label.col-md-2.control-label 帳號
                .col-md-10
                  p.form-control-static email@example.com
              // /form-group
              .form-group
                label.col-md-2.control-label 密碼
                .col-md-10
                  input.form-control(type='password', placeholder='Password')
              // /form-group
              .row.m-top-6
                .col-sm-6.col-sm-offset-3
                  a.btn.btn-black.btn-block.border-radius-circle(href='index-slider') 登入
                // /col-sm-6
              // /row
          // /border-block-3
        // /col-sm-6
      // /row
    // /container

    include ../partial/_auth_footer

